{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AiEventSettings": {
    "Model": "gpt-3.5-turbo",
    "HttpTimeout": 30,
    "RetrySettings": {
      "MaxRetryCount": 3,
      "Delay": 1,
      "MaxDelay": 10,
      "Jitter": 5
    },
    "CircuitBreakerSettings": {
      "Enabled": true,
      "DurationOfBreak": 30,
      "FailureThreshold": 5,
      "MinimumThroughput": 10
    }
  }
}
